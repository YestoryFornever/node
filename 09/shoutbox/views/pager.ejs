<div id="pager">
	<% if (page.count >1) {%>
		<% if (page.number) {%>
			<a href="./<%= page.number %>" id="prev"><</a>
		<% } %>
		<% if (page.number < page.count - 1) {%>
			<% if(page.number){ %>
				&nbsp;&nbsp;
			<% } %>
			<a href="./<%= page.number + 2 %>" id="next">></a>
		<% } %>
	<% } %>
</div>